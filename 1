per_cent = {'TKB': 5.6, 'CKB': 5.9, 'VTB': 4.28, 'CBER': 4.0}
money=float(input('Введите сумму:'))
TKB=int((per_cent["TKB"])*(money/100))
CKB=int((per_cent["CKB"])*(money/100))
VTB=int((per_cent["VTB"])*(money/100))
CBER=int((per_cent["CBER"])*(money/100))
deposit={"TKB":TKB, "CKB":CKB,"VTB":VTB,"CBER":CBER}
print(deposit)
max_val=max(deposit.values())
print("Максимальный депозит:",max_val)
